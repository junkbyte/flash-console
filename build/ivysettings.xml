<?xml version="1.0" encoding="UTF-8"?>
<ivysettings>
    <settings defaultResolver="main" />
    <!--Authentication required for publishing (deployment). 'Artifactory Realm' is the realm used by Artifactory so don't change it to
	<credentials host="as-artifacts.org" realm="Artifactory Realm" username="???" passwd="???"/> -->
    <credentials host="as-artifacts.org" realm="Artifactory Realm" />
    <resolvers>
        <chain name="main">
            <ibiblio name="as-artifacts" m2compatible="true" root="http://as-artifacts.org/flash-console" />
        </chain>
    </resolvers>
</ivysettings>